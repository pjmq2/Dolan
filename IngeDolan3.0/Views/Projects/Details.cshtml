@model IngeDolan3._0.Models.Project

@{
    ViewBag.Title = "Detailles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles de Proyecto</h2>

<div class="navbar-right" style="display:inline-block;">
    @Html.ActionLink("Editar", "Edit", new { id = Model.ProjectID }, new { @class = "btn btn-default" })
    @Html.ActionLink("Borrar", "Delete", new { id = Model.ProjectID }, new { @class = "btn btn-default" })
</div>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre del Proyecto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProjectName)
        </dd>

        <dt>
            Descripcion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Descriptions)
        </dd>

        <dt>
            Lider del proyecto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.User.name)
        </dd>

        <dt>
            Fecha de Inicio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StartingDate)
        </dd>

        <dt>
            Fecha Final
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FinalDate)
        </dd>

        <div>
            @Html.ActionLink("Volver a la lista", "Index")
        </div>
    </dl>
</div>

