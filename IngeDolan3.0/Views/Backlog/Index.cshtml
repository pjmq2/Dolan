@model IngeDolan3._0.Models.ProyectoList
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Product Backlog </h2>
<div class="ddlm">
    <div class="col-md-8">
        <section id="productBacklogForm">
            @using (Html.BeginForm("Index", "UserStories", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", ProjectName = "form" }))
            {
                @Html.AntiForgeryToken()
                <h4>Seleccione el proyecto.</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @*@Html.LabelFor(model => model.ProjectName, "Nombre Proyecto", htmlAttributes: new { @class = "control-label col-md-2" })*@
                    <div class="col-md-10">
                        @*@Html.DropDownListFor(model=>model.ProjectID, (SelectList)ViewBag.proyectList, htmlAttributes: new { @class = "form-control" })*@
                        @Html.DropDownListFor(model => model.id, Model.Nombres, htmlAttributes: new { @class = "form-control" })
                        @*@Html.DropDownList("ProjectName", null, htmlAttributes: new { @class = "form-control" })*@
                    </div>
                 </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Aceptar" class="btn btn-default" />
                        <input type="reset" value="Cancelar" class="btn btn-default" />
                    </div>
                </div>
                }
            </section>
        </div>
    </div>

    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }