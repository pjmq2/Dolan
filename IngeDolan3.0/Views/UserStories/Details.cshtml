@model IngeDolan3._0.Models.UserStory

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div class="navbar-right" style="display:inline-block;">
    @Html.ActionLink("Editar", "Edit", new { storyId = Model.StoryID, projectId = Model.ProjectID }, new { @class = "btn btn-default" })
    @Html.ActionLink("Borrar", "Delete", new { storyId = Model.StoryID, projectId = Model.ProjectID }, new { @class = "btn btn-default" })
</div>

<div>
    <h4>Historia de Usuario</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id de la historia de Usuario
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SprintID)
        </dd>

        <dt>
            Alias
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Alias)
        </dd>

        <dt>
            Rol del cliente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ClientRole)
        </dd>

        <dt>
            Descripcion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Funtionality)
        </dd>

        <dt>
            Razon
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Reason)
        </dd>

        <dt>
            Prioridad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Priorities)
        </dd>

        <dt>
            Estimacion de la Historia
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estimation)
        </dd>

        <dt>
            Proyecto al que pertenece
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Project.Descriptions)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Regresar", "Index")
</p>
